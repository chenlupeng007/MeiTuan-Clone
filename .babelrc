{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@client": "./src/client/",
        "@server": "./src/server/",
        "@components": "./src/client/components",
        "@reducers": "./src/client/reducers",
        "@webpack": "./webpack.config.babel.js"
      }
    }]
  ]
}